{
  "name": "chai-struct",
  "description": "Simple, readable, structural type comparisons for Chai",
  "version": "3.0.3",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src/",
    "test": "mocha -r babel-register src/*.test.js",
    "tdd": "npm t -s -- -w -R min",
    "coverage": "babel-node node_modules/.bin/babel-istanbul cover _mocha --",
    "cover": "rimraf coverage/ && run-s -s 'coverage {@}' -- src/*.test.js",
    "transpile": "rimraf dist/ && babel -d dist/ --ignore *.test.js src/",
    "precommit": "run-s -s lint cover transpile"
  },
  "files": [
    "dist/",
    "index.d.ts"
  ],
  "engines": {
    "node": ">=6.3.1",
    "npm": ">=3.10.3"
  },
  "keywords": [
    "object",
    "schema",
    "types",
    "chai",
    "plugin",
    "structure"
  ],
  "author": "Tim Davis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thebearingedge/chai-struct.git"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.4",
    "babel-istanbul": "0.12.2",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-preset-node6": "11.0.0",
    "chai": "4.1.2",
    "eslint": "5.0.0",
    "coveralls": "3.0.1",
    "global": "4.3.2",
    "husky": "2.2.0",
    "mocha": "5.2.0",
    "npm-run-all": "4.1.3",
    "rimraf": "2.6.2"
  },
  "dependencies": {
    "type-diff": "3.1.1"
  }
}
